{
    "name": "zupaazhai/imager",
    "description": "Blade component for image displaying",
    "keywords": [
        "zupaazhai",
        "laravel",
        "imager"
    ],
    "homepage": "https://github.com/zupaazhai/imager",
    "license": "MIT",
    "authors": [
        {
            "name": "Supachai Wongmoon",
            "email": "supachai@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Zupaazhai\\Imager\\": "src",
            "Zupaazhai\\Imager\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zupaazhai\\Imager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zupaazhai\\Imager\\ImagerServiceProvider"
            ],
            "aliases": {
                "Imager": "Zupaazhai\\Imager\\ImagerFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
